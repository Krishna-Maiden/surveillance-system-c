{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CloudProvider": {
    "Provider": "AWS",
    "AWS": {
      "Region": "us-east-1",
      "Rekognition": {
        "AccessKeyId": "<your-access-key>",
        "SecretAccessKey": "<your-secret-key>",
        "S3Bucket": "<your-s3-bucket>"
      }
    },
    "Azure": {
      "CognitiveServices": {
        "Endpoint": "<your-cognitive-endpoint>",
        "ApiKey": "<your-cognitive-key>"
      },
      "VideoIndexer": {
        "AccountId": "<your-account-id>",
        "Location": "<your-location>",
        "ApiKey": "<your-video-indexer-key>"
      }
    }
  }
}
